    "plt.rcParams['axes.unicode_minus'] = False\n",
    "\n",
    "# 示例绘图\n",
    "t = np.linspace(0, 1, 100)\n",
    "x = t**2\n",
    "y = t**3\n",
    "plt.plot(x, y, label=\"Bezier curve example\")  # 使用英文标签\n",
    "plt.legend()\n",
    "plt.show()\n",
    "plt.close()"
   ]
